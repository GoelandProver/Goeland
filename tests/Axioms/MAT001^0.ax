thf('3d.def_plane_equality4_2_axiom',axiom,
    ! [V_n: '3d.Vector',V_P: '3d.Point',V_v: $real,V_u: $real,V_t: $real,V_s: $real] :
      ( ( ( '3d.plane-type/1' @ ( '3d.plane/4' @ V_s @ V_t @ V_u @ V_v ) )
