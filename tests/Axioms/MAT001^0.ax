    <=> ( $less @ 0.0 @ V_a ) ) ).

thf('.def-fun-convex-upward-in-region_axiom',axiom,
    ! [V_f: 'R2R',V_region: 'SetOf' @ $real] :
      ( ( 'fun-convex-upward/2' @ V_f @ V_region )
    <=> ! [V_x1: $real,V_x2: $real] :
          ( ( ( 'elem/2' @ $real @ V_x1 @ V_region )
