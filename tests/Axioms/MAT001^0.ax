      = ( 'impl/2' @ ( 'not/1' @ V_q ) @ ( 'not/1' @ V_p ) ) ) ).

thf('.def-converse-of_axiom',axiom,
    ! [V_p: 'Prop',V_q: 'Prop'] :
      ( ( 'converse-of/1' @ ( 'impl/2' @ V_p @ V_q ) )
      = ( 'impl/2' @ V_q @ V_p ) ) ).

