            | ( ( $less @ V_hi @ ( $uminus @ ( $quotient @ V_b @ ( $product @ 2.0 @ V_a ) ) ) )
              & ( V_xm = V_hi )
              & ( V_ym
                = ( 'poly-value/2' @ ( 'cons/2' @ $real @ V_c @ ( 'cons/2' @ $real @ V_b @ ( 'cons/2' @ $real @ V_a @ ( 'nil/0' @ $real ) ) ) ) @ V_hi ) ) ) ) )
