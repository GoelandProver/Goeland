thf('stat.def-uniq-rec_axiom',axiom,
    ! [V_v1: $real,V_v2: $real,V_vs: 'ListOf' @ $real] :
      ( ( 'stat.uniq/1' @ ( 'cons/2' @ $real @ V_v1 @ ( 'cons/2' @ $real @ V_v2 @ V_vs ) ) )
      = ( 'if/3' @ ( 'ListOf' @ $real )
        @ ^ [V__: 'Unit'] : ( V_v1 = V_v2 )
