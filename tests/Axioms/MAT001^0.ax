thf('stat.def-correlation-table-equality_axiom',axiom,
    ! [V_dist1: 'ListOf' @ ( 'Triple' @ 'stat.Class' @ 'stat.Class' @ $real ),V_dist2: 'ListOf' @ ( 'Triple' @ 'stat.Class' @ 'stat.Class' @ $real )] :
