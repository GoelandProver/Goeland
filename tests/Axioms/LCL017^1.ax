thf(eiw_ind,type,
    eiw_ind: $i > mu > $o ).

thf(nonempty_ind,axiom,
    ! [V: $i] :
    ? [X: mu] : ( eiw_ind @ V @ X ) ).

thf(mforall_eiw_ind_type,type,
    mforall_eiw_ind: ( mu > $i > $o ) > $i > $o ).

thf(mforall_eiw_ind,definition,
    ( mforall_eiw_ind
    = ( ^ [Phi: mu > $i > $o,W: $i] :
        ! [X: mu] :
          ( ( eiw_ind @ W @ X )
         => ( Phi @ X @ W ) ) ) ) ).

thf(mexists_eiw_ind_type,type,
    mexists_eiw_ind: ( mu > $i > $o ) > $i > $o ).

thf(mexists_eiw_ind,definition,
    ( mexists_eiw_ind
    = ( ^ [Phi: mu > $i > $o] :
          ( mnot
          @ ( mforall_eiw_ind
            @ ^ [X: mu] : ( mnot @ ( Phi @ X ) ) ) ) ) ) ).

